	//Ejemplo05
	//Solicitar 2 números y mostrar la suma de aMbos
	
	//DECLARACIÓN Y ASIGNACIÓN DE DATOS
	definir X , A , Z , B , resultado Como Real;
	
	//ENTRADA
	escribir "Digite el valor del primer número: " ;
	leer A;
	escribir "Digite el valor del segundo número: " ;
	leer B;
	
	//PROCESO, FÓRMULAS ETC
	x <- A;
	z <- B;
	resultado <- X + Z;
	
	//SALIDA DE DATOS
	escribir "El resultado de sumar " , x , " + " , z , " es igual a: " , resultado;
FinProceso